msgstr "mögliche Produktvarianten"

msgid "normal"
msgstr "normal"

msgid "package parent"
msgstr "übergeordnete Produktpackung"

msgid "variation parent (simple)"
msgstr "übergeordnete Produktvariante (einfach)"

msgid "variation parent (variable)"
msgstr "übergeordnete Produktvariante (variabel)"

msgid "variation child"
msgstr "untergeordnete Produktvariante"

msgid "subscription"
msgstr "Abonnement"

msgid "visible to logged in"
msgstr "für angemeldete Benutzer sichtbar"

msgid "visible to groups"
msgstr "für Gruppen sichtbar"

msgid "stocked"
msgstr "auf Lager"

msgid "unstocked"
msgstr "nicht vorrätig"

msgid "recommended"
msgstr "empfohlene"

msgid "related"
msgstr "passende"

