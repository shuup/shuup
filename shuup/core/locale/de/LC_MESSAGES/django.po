msgid "Quantity %s is invalid."
msgstr "Menge %s ist ungültig."

msgid "primary product"
msgstr "Hauptprodukt"

msgid "secondary product"
msgstr "Nebenprodukt"

msgid "weight"
msgstr "Gewicht"

msgid "cross sell link"
msgstr "Cross-Selling-Verbindung"

msgid "cross sell links"
msgstr "Cross-Selling-Verbindungen"

