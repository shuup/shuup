msgid ""
"<p>Dear {{ order.customer }},</p>\n"
"<p>A shipment has been created for your order and here are the details:</p>\n"
"\n"
"{% if shipment.tracking_code %}\n"
"<p>Tracking code: {{ shipment.tracking_code }}</p>\n"
"{% endif %}\n"
"\n"
"<p>Contents:</p>\n"
"<table>\n"
"    <thead>\n"
"        <tr>\n"
"            <th>Product</th>\n"
"            <th>Quantity</th>\n"
"        </tr>\n"
"    </thead>\n"
"    <tbody>\n"
"{% for shipment_product in shipments.total_products %}\n"
"        <tr>\n"
"            <td>{{ shipment_product.product }}</td>\n"
"            <td>{{ shipment_product.quantity }}</td>\n"
"        </tr>\n"
"{% endif %}\n"
"    </tbody>\n"
"</table>\n"
msgstr "<p> Hej {{ order.customer }},</p>\n<p>En leverans har skapats för din beställning, här är detaljer angående leveransen:</p>\n\n{% if shipment.tracking_code %}\n<p>Spårningskod: {{ shipment.tracking_code }}</p>\n{% endif %}\n\n<p>Innehåll:</p>\n<table>\n    <thead>\n        <tr>\n            <th>Produkt</th>\n            <th>Antal</th>\n        </tr>\n    </thead>\n    <tbody>\n{% for shipment_product in shipments.total_products %}\n        <tr>\n            <td>{{ shipment_product.product }}</td>\n            <td>{{ shipment_product.quantity }}</td>\n        </tr>\n{% endif %}\n    </tbody>\n</table>\n"

