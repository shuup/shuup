{
  "name": "shuup.admin",
  "version": "2.0.0",
  "description": "Shuup Team <shuup@shuup.com>",
  "main": "",
  "scripts": {
    "watch:styles:base": "parcel watch --no-hmr -d static/shuup_admin/css static_src/base/scss/style.scss -o base",
    "watch:styles:dashboard": "parcel watch --no-hmr -d static/shuup_admin/css static_src/dashboard/scss/dashboard.scss",
    "watch:styles:wizard": "parcel watch --no-hmr -d static/shuup_admin/css static_src/wizard/less/wizard.scss",
    "watch:styles:media": "parcel watch --no-hmr -d static/shuup_admin/css modules/media/static_src/media/browser/media-browser.scss",
    "watch:scripts:base": "parcel watch --no-hmr -d static/shuup_admin/js static_src/base/js/base.js",
    "watch:scripts:contact_group": "parcel watch --no-hmr -d static/shuup_admin/js static_src/contact_group/edit-members.js -o contact-group",
    "watch:scripts:dashboard": "parcel watch --no-hmr -d static/shuup_admin/js static_src/dashboard/js/dashboard.js --global Chart",
    "watch:scripts:media": "parcel watch --no-hmr -d static/shuup_admin/js modules/media/static_src/media/browser/Browser.js -o media-browser --global MediaBrowser",
    "watch:scripts:orders": "parcel watch --no-hmr -d static/shuup_admin/js modules/orders/static_src/create/index.js -o order-creator.js --global OrderCreator",
    "watch:scripts:picotable": "parcel watch --no-hmr -d static/shuup_admin/js static_src/picotable/picotable.js --global Picotable",
    "watch:scripts:product": "parcel watch --no-hmr -d static/shuup_admin/js static_src/product/edit_media.js -o product",
    "watch:scripts:product_var": "parcel watch --no-hmr -d static/shuup_admin/js modules/products/static_src/product-variation-variable-editor.js",
    "watch:scripts:vendor": "parcel watch --no-hmr -d static/shuup_admin/js static_src/vendor/vendor.js",
    "watch:scripts:wizard": "parcel watch --no-hmr -d static/shuup_admin/js static_src/wizard/js/wizard.js",
    "watch": "concurrently --kill-others-on-fail npm:watch:*"
  },
  "shuup": {
    "static_build": true
  },
  "author": "Shuup Team <support@shuup.com>",
  "license": "OSL-3.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.2",
    "chart.js": "^2.7.2",
    "desandro-matches-selector": "^2.0.2",
    "dropzone": "^5.5.1",
    "ev-emitter": "^1.1.1",
    "fizzy-ui-utils": "^2.0.7",
    "font-awesome": "^4.7.0",
    "get-size": "^2.0.3",
    "imagelightbox": "^0.10.0",
    "jquery": ">=1.0.0",
    "jquery.easing": "^1.4.1",
    "jquery.scrollbar": "^0.2.11",
    "lodash": "^4.17.10",
    "masonry-layout": "^4.2.2",
    "mithril": "^0.2.0",
    "node-sass": "^4.9.2",
    "outlayer": "^2.1.1",
    "popper": "^1.0.1",
    "redux": "^3.0.4",
    "redux-actions": "^0.9.0",
    "redux-persist": "^3.5.0",
    "select2": "^3.5.1",
    "select2-bootstrap-css": "^1.4.6",
    "sortablejs": "^1.7.0",
    "summernote": "^0.8.10",
    "tether": "^1.4.4",
    "tether-shepherd": "^1.8.1",
    "tooltip.js": "^1.2.0",
    "vinyl-source-stream": "^2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.5",
    "babel-core": "^6.26.3",
    "babel-plugin-wildcard": "^5.0.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^3.6.0",
    "eslint": "^5.1.0",
    "parcel-bundler": "^1.9.6",
    "postcss-modules": "^1.1.0"
  }
}
