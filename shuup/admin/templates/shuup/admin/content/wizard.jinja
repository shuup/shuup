<div class="form-def" data-name="{{ pane.identifier }}" id="{{ pane.identifier }}-content-form">
    <div class="row">
        <div class="panel panel-default col-md-6 offset-md-3">
            <h2>{{ form_def.context.title }}</h2>
            {% for field in pane.forms[form_def.name] %}
                {{ bs3.field(field) }}
            {% endfor %}
        </div>
    </div>
</div>
