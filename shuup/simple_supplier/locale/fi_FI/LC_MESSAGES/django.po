#, python-format
msgid ""
"Added %(delta)s %(unit_short_name)s for product %(product_name)s stock "
"(%(supplier_name)s)"
msgstr ""
"Lisättiin %(delta)s %(unit_short_name)s tuotteelle %(product_name)s "
"varastoon (%(supplier_name)s)"

