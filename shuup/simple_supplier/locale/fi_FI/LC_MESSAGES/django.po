msgid "Physical count: %(count)s %(sales_unit)s"
msgstr "Fyysinen määrä: %(count)s %(sales_unit)s"

#, python-format
msgid "Logical count: %(count)s %(sales_unit)s"
msgstr "Looginen määrä: %(count)s %(sales_unit)s"

#, python-format
msgid "Value per unit: %(value)s"
msgstr "Arvo per yksikkö: %(value)s"

#, python-format
