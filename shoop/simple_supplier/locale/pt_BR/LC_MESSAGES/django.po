msgid "Stock management"
msgstr "Gerenciamento do estoque"

msgid "Products"
msgstr "Produtos"

msgid "SKU"
msgstr "SKU"

msgid "Filter by SKU..."
msgstr "Filtrar por SKU..."

msgid "Name"
msgstr "Nome"

msgid "Filter by name..."
msgstr "Filtrar por nome..."

msgid "Supplier"
msgstr "Fornecedor"

msgid "Stock information"
msgstr "Informações do estoque"

msgid "Adjust stock"
msgstr "Ajustar estoque"

#, python-format
msgid ""
"Added %(delta)s %(unit_short_name)s for product %(product_name)s stock "
"(%(supplier_name)s)"
msgstr "Adicionado %(delta)s %(unit_short_name)s do produto %(product_name)s ao estoque (%(supplier_name)s)"

#, python-format
msgid ""
"Removed %(delta)s %(unit_short_name)s from product %(product_name)s stock "
"(%(supplier_name)s)"
msgstr "Removido %(delta)s %(unit_short_name)s do produto %(product_name)s do estoque (%(supplier_name)s)"

msgid "Not allowed"
msgstr "Não permitido"

msgid "Error, please check submitted values and try again."
msgstr "Erro, por favor, verifique os valores enviados e tente novamente."

#, python-format
msgid "Error, please check submitted values and try again (%(error)s)."
msgstr "Erro, por favor, verifique os valores enviados e tente novamente (%(error)s)."

#, python-format
msgid "Purchase price per unit (%(currency_name)s)"
msgstr "Preço de compra por unidade (%(currency_name)s)"

msgid "Quantity"
msgstr "Quantidade"

msgid "Only non-zero values can be added to stock."
msgstr "Apenas valores acima de zero podem ser adicionados ao estoque."

msgid "product"
msgstr "produto"

msgid "supplier"
msgstr "fornecedor"

msgid "created on"
msgstr "criado em"

msgid "created by"
msgstr "criado por"

msgid "delta"
msgstr "delta"

msgid "logical count"
msgstr "contagem lógica"

msgid "physical count"
msgstr "contagem física"

msgid "Add stock"
msgstr "Adicionar estoque"

#, python-format
msgid ""
"Product <a href=\"%(product_url)s\" target=\"_blank\">%(product_name)s</a> "
"does not have suppliers with %(module_name)s module."
msgstr "Produto <a href=\"%(product_url)s\" target=\"_blank\">%(product_name)s</a> não tem fornecedores com o módulo %(module_name)s."

msgid "Please set suppliers at Purchasing section."
msgstr "Por favor, defina os fornecedores na seção Compras."

#, python-format
msgid ""
"Product <a href=\"%(product_url)s\" target=\"_blank\">%(product_name)s</a> "
"is not stocked."
msgstr "Produto <a href=\"%(product_url)s\" target=\"_blank\">%(product_name)s</a> não é estocado."

msgid "Please set product stock behavior at Additional Details section."
msgstr "Por favor, defina o comportamento do estoque do produto na seção Detalhes Adicionais."

#, python-format
msgid "No suppliers with %(module_name)s available."
msgstr "Não há fornecedores com o módulo %(module_name)s disponível."

#, python-format
msgid ""
"Please set %(module_name)s module for at least one of the <a "
"href=\"%(supplier_url)s\" target=\"_blank\">Suppliers</a>."
msgstr "Por favor, defina o módulo %(module_name)s para pelo menos um dos seus <a href=\"%(supplier_url)s\" target=\"_blank\">Fornecedores</a>."

msgid "Physical count"
msgstr "Contagem física"

msgid "Logical count"
msgstr "Contagem lógica"

msgid "Value per unit"
msgstr "Valor por unidade"

msgid "Total value"
msgstr "Valor total"

msgid "No stock information available."
msgstr "Nenhuma informação do estoque disponível."
